{
  "name": "bark-coding",
  "version": "0.2",
  "description": "A Fun block-based coding editor similar to Scratch.",
  "scripts": {
    "dev": "nw editor/",
    "prod:win": "nwbuild --mode build --version 0.89.0  editor/  --platform win --glob false",
    "prod:linux": "nwbuild --mode build --version 0.89.0  editor/ --platform linux --glob false",
    "prod:osx": "nwbuild --mode build --version 0.89.0  editor/ --platform osx --glob false",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "main": "index.html",
  "dependencies": {
    "express": "^4.19.2",
    "nw": "^0.89.0"
  },
  "devDependencies": {
    "nw-builder": "^4.7.7",
    "prettier": "^3.3.2"
  }
}
